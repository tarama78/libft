/* ************************************************************************** */

#include <libft.h>

void		ft_putendl(const char *s)
{
	ft_putstr(s);
	ft_putchar('\n');
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_putendl_fd.c                                    :+:      :+:    :+:   */
